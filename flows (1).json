        "id": "41dddbb33560bf73",
        "type": "function",
        "z": "2724c09f2d6a4f7f",
        "name": "function 3",
        "func": "// Busca as Ãºltimas 20 leituras da tabela sensor_data\nmsg.topic = \"SELECT `temperatura`, `umidade`, `timestamp` FROM sensor_data ORDER BY id DESC LIMIT 20;\";\n\nreturn msg;",
